-lib hxparse
-lib assertion
-lib literals
-lib sys-utils
-lib version
-cp include
-cp src

--each

-main Main
-neko bin/obrt.n
-cmd nekotools boot bin/obrt.n

--next

-main Main
-js bin/obrt.js
-lib hxnodejs
# -D source-map-content
-debug

--next

-main tools.Stats
-js bin/obrt-stats.js
-lib hxnodejs

--next

-main RunTests
-js bin/tests.js
-cp test
-lib utest
-cp pseudo
--macro allowPackage("sys")
-D hxnodejs

--next

-main RunTests
-neko bin/tests.n
-cp test
-lib utest
-cp pseudo

